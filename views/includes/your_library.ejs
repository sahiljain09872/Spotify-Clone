<div style="background-color: rgba(0, 0, 0, 0.5)" class="content-library">
  <h1>Your libraries</h1>

  <div class="libraries">

    <div class="search_library_recent_artists">
      <div class="recent_search">
        <button
          class="icon hover_effect full_opac search_library"
          id="search_library"
        >
          <i class="fa-solid fa-magnifying-glass"></i>
        </button>

        <button class="recent full_opac" id="recent">
          <span>Recent</span>
          <div class="recent_icon icon">
            <i class="fa-solid fa-ellipsis-vertical" style="margin: 2px"></i>
            <i class="fa-solid fa-bars"></i>
          </div>
        </button>
      </div>
      
      </div>
    </div>
    <%- include("libraries.ejs" , {libraries}) %>
  </div>
</div>
